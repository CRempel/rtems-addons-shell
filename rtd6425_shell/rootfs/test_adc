#! joel

echo "*** assume dac 4 is connected to ADC 0 ***"
echo "Cycle from -5v to +5v with one second between on dac 4"

multiio_dac 4 -5.0
sleep 1
multiio_adc 0

multiio_dac 4 -4.0
sleep 1
multiio_adc 0

multiio_dac 4 -3.0
sleep 1
multiio_adc 0

multiio_dac 4 -2.0
sleep 1
multiio_adc 0

multiio_dac 4 -1.0
sleep 1
multiio_adc 0

multiio_dac 4 0.0
sleep 1
multiio_adc 0

multiio_dac 4 1.0
sleep 1
multiio_adc 0

multiio_dac 4 2.0
sleep 1
multiio_adc 0

multiio_dac 4 3.0
sleep 1
multiio_adc 0

multiio_dac 4 4.0
sleep 1
multiio_adc 0

multiio_dac 4 5.0
sleep 1
multiio_adc 0

echo "Write 0.0v to dac 4"
multiio_dac 4 0.0
sleep 1
multiio_adc 0

